{
  "name": "db",
  "version": "0.0.0",
  "license": "MIT",
  "private": true,
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "format": "prettier --write ./index.ts",
    "lint": "eslint --fix ./index.ts",
    "typecheck": "tsc --noEmit",
    "db:generate": "prisma generate",
    "db:studio": "yarn prisma studio",
    "db:push": "yarn prisma db push --skip-generate",
    "clean": "rimraf .turbo/ dist/"
  },
  "devDependencies": {
    "@joogie/eslint-config": "^0.1.0",
    "@joogie/prettier-config": "^0.1.0",
    "@joogie/tailwind-config": "^0.1.0",
    "@joogie/tsconfig": "^0.1.0",
    "@types/node": "^20.1.7",
    "prisma": "^4.14.0",
    "typescript": ">4.5.0"
  },
  "dependencies": {
    "@prisma/client": "^4.14.0"
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "@joogie/eslint-config/base",
      "@joogie/eslint-config/nextjs",
      "@joogie/eslint-config/react"
    ]
  },
  "prettier": "@joogie/prettier-config"
}
